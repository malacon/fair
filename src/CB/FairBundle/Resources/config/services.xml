<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="form.type.datePicker" class="CB\FairBundle\Form\Type\DatePickerType">
            <tag name="form.type" alias="datePicker" />
        </service>
        <service id="form.type.dateTimePicker" class="CB\FairBundle\Form\Type\DateTimePickerType">
            <tag name="form.type" alias="dateTimePicker" />
        </service>
    </services>
</container>